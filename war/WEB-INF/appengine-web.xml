<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>pushboard</application>
	<version>1</version>

	<sessions-enabled>true</sessions-enabled>

	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
	</system-properties>

	<static-files>
		<include path="/_assets/images/favicon.ico"/>
		<include path="/_assets/**.css"/>
		<include path="/_assets/**.js"/>
		<!--<exclude path="/*.tpl.html"/>-->
	</static-files>

	<resource-files>
		<!--<include path="/*.tpl.html"/>-->
	</resource-files>
</appengine-web-app>